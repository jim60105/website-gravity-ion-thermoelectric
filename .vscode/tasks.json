{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "start",
			"type": "shell",
			"command": "./node_modules/.bin/http-server",
			"args": [
				"-p",
				"3000"
			],
			"isBackground": true,
			"group": "build"
		},
		{
			"label": "install",
			"type": "shell",
			"command": "npm",
			"args": [
				"install"
			],
			"group": "build"
		}
	]
}
